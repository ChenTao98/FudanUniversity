<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
    <servlet-name>DetailServlet</servlet-name>
    <servlet-class>artStore.servlet.DetailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>DetailServlet</servlet-name>
    <url-pattern>/detail</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ArtistDetailServlet</servlet-name>
        <servlet-class>artStore.servlet.ArtistDetailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ArtistDetailServlet</servlet-name>
        <url-pattern>/artistDetail</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CartDeleteServlet</servlet-name>
        <servlet-class>artStore.servlet.CartDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CartDeleteServlet</servlet-name>
        <url-pattern>/CartDeleteServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SignOutServlet</servlet-name>
        <servlet-class>artStore.servlet.SignOutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignOutServlet</servlet-name>
        <url-pattern>/SignOutServlet</url-pattern>
    </servlet-mapping>
</web-app>